    "@mui/material": "^5.11.8",
    "@types/markdown-it": "^12.2.3",
    "@types/store": "^2.0.2",
    "@types/url-parse": "^1.4.8",
    "antd": "4.24.7",
    "axios": "^1.3.2",
    "classnames": "^2.3.2",
    "htmlparser2": "^9.0.0",
    "markdown-it": "^13.0.1",
